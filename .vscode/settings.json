// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/*.js": {"when": "$(basename).ts"},
        "**/*.map": true,
        "node_modules": true,
        "e2e": true,
        ".editorconfig": true,
        ".gitignore": true,
        ".angular-cli.json": true,
        "karma.conf.js": true,
        "protractor.conf.js": true,
        "README.md": true,
        "**/tsconfig*.json": true,
        "package.json": true,
        "tslint.json": true,
        "**/*spec.*": true,
        "**/.gitkeep": true,
        "**/test.ts": true,
        "**/typings.d.ts": true,
        "**/environments": true,
        "**/assets": true,
        "**/favicon.ico": true,
        "**/polyfills.ts": true
    }
,
"typescript.tsdk": "./node_modules/typescript/lib"
}